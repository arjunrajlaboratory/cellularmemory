# Function to process the data
process_data <- function(file_path) {
data <- read.csv(file_path)
# Rename columns
names(data)[names(data) == "cell Blob Blob metrics / Area"] <- "Area"
names(data)[names(data) == "cell Blob Parent and child / parentId"] <- "ParentId"
names(data)[names(data) == "cell Blob Parent and child / childId"] <- "ChildId"
# Convert ParentId and ChildId to string
data$ParentId <- as.character(data$ParentId)
data$ChildId <- as.character(data$ChildId)
# Track lineages
data$Lineage <- NA
starting_points <- data[data$ParentId == '-', ]
for (i in 1:nrow(starting_points)) {
current_id <- starting_points$ChildId[i]
lineage_id <- starting_points$Id[i]
data$Lineage[data$ChildId == current_id] <- lineage_id
while (any(data$ParentId == current_id, na.rm = TRUE)) {
next_row <- data[data$ParentId == current_id, ]
if (nrow(next_row) > 0) {
current_id <- next_row$ChildId[1]
data$Lineage[data$ChildId == current_id] <- lineage_id
}
}
}
# Remove rows with NA in Lineage
data <- na.omit(data)
return(data)
}
# Process each file
data_list <- lapply(file_paths, process_data)
